<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="security.authentication.listener.totp.class">Dayspring\PBKDF2Bundle\Security\Http\Firewall\TOTPUsernamePasswordFormAuthenticationListener</parameter>

	</parameters>
	<services>
		<service id="security.authentication.listener.totp"
                 class="%security.authentication.listener.totp.class%"
                 parent="security.authentication.listener.abstract"
                 abstract="true" />		
		
	</services>
</container>
